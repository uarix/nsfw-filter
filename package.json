{
  "name": "nsfw-filter",
  "version": "2.3.0",
  "description": "Free, open source, and privacy-focused browser extension to block \"not safe for work\" content.",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist/src",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "cross-env HEADLESS=false jest --no-cache -c ./test/jest.e2e.config.js",
    "test:unit": "jest --no-cache -c ./test/jest.unit.config.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "build:zip": "web-ext build --source-dir dist/ --artifacts-dir ./builds --overwrite-dest", 
    "dev": "webpack --config webpack/webpack.dev.js",
    "dev:chrome": "npm-run-all --parallel dev start:chrome",
    "start:chrome": "web-ext run --source-dir ./dist/ --target=chromium",
    "dev:edge": "npm-run-all --parallel dev start:edge",
    "start:edge": "web-ext run --source-dir ./dist/ --target=chromium --chromium-binary=\"C:\\Program Files (x86)\\Microsoft\\Edge\\Application\\msedge.exe\""
  },
  "author": {
    "name": "Navendu Pottekkat",
    "email": "navendu@apache.org",
    "url": "https://navendu.me"
  },
  "contributors": [
    {
      "name": "Yegor Zaremba",
      "url": "https://github.com/yegorzaremba"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nsfw-filter/nsfw-filter"
  },
  "homepage": "https://github.com/nsfw-filter/nsfw-filter",
  "license": "GPL-3.0-only",
  "dependencies": {
    "antd": "5.20.0",
    "nsfwjs": "4.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "reduxed-chrome-storage": "2.0.0",
    "styled-components": "5.2.0"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "4.20.0",
    "@types/antd": "1.0.4",
    "@types/chrome": "0.0.122",
    "@types/jest": "29.5.14",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/styled-components": "5.1.3",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "antd-dayjs-webpack-plugin": "1.0.6",
    "copy-webpack-plugin": "12.0.2",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "eslint": "7.5.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-standard-with-typescript": "18.0.2",
    "eslint-plugin-better-styled-components": "1.1.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-import-helpers": "1.1.0",
    "eslint-plugin-n": "14.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-standard": "4.0.1",
    "html-webpack-plugin": "5.6.3",
    "jest": "29.7.0",
    "mini-css-extract-plugin": "2.9.2",
    "mkdirp": "3.0.1",
    "npm-run-all2": "5.0.2",
    "postcss-loader": "8.1.1",
    "puppeteer": "22.15.0",
    "purgecss-webpack-plugin": "6.0.0",
    "resolve-url-loader": "3.1.5",
    "sass": "1.77.8",
    "sass-loader": "16.0.4",
    "style-loader": "4.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "typescript": "5.5.4",
    "web-ext": "8.2.0",
    "webpack": "5.97.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-ext-reloader": "1.1.13",
    "webpack-merge": "6.0.1"
  }
}
